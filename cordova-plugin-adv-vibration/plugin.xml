<?xml version="1.0" encoding="UTF-8"?>
<plugin id="com.example.advvibration" version="1.0.0"
  xmlns="http://apache.org/cordova/ns/plugins"
  xmlns:android="http://schemas.android.com/apk/res/android">
  <name>AdvVibration</name>

  <js-module name="AdvVibration" src="www/advVibration.js">
    <clobbers target="AdvVibration"/>
  </js-module>

  <platform name="android">
    <config-file target="AndroidManifest.xml" parent="/manifest">
      <uses-permission android:name="android.permission.VIBRATE"/>
    </config-file>
    <source-file src="src/android/AdvVibration.java" target-dir="src/com/example/advvibration"/>
  </platform>
</plugin>
